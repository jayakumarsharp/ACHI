<form novalidate role="form" name="form3" ng-show="pageList[2].Page">
    <div class="col-lg-12">
        <div class="form-group  row ">
            <div class="col-sm-5" style="margin-left:15px;">
                <div class="list-group box-comments" id="list1Estimation" style="height:250px;overflow-x:auto">
                    <a href="javascript:;" class="list-group-item active grouppadding">
                        Available Approvers
                        <input style="color:black;margin-left:5px;" ng-model="search.Approver" />
                    </a>
                    <span ng-repeat="right in listA_Estimation | filter:{Approver: search.Approver}">
                        <a href="javascript:;" data-id="{{right.Id}}" class="list-group-item grouppadding">
                            {{right.Approver}}
                            <input ng-model="isCheckedA_Estimation" ng-click="stateAChanged_Estimation(isCheckedA_Estimation, right.Approver)" name="selectedA_Estimation[]" value="{{right.Approver}}" class="pull-right" type="checkbox" ng-true-value="true" ng-false-value="false">
                        </a>
                    </span>
                </div>
            </div>
            <div class="col-md-1 v-center text-center">
                <br />
                <br />
                <br />
                <div class="btn-group-vertical">
                    <button class="btn btn-primary" style="min-width: 9px;padding :13px 4px;" ng-click="AlltoA_Estimation()" id="btnAllA_Estimation"><i class="fa fa-angle-double-left"></i></button>
                    <button class="btn btn-primary" style="min-width: 9px;padding :13px 4px;" ng-click="bToA_Estimation()" id="btnA_Estimation"><i class="fa fa-angle-left"></i></button>
                    <button class="btn btn-primary" style="min-width: 9px;padding :13px 4px;" ng-click="aToB_Estimation()" id="btnB_Estimation"><i class="fa fa-angle-right"></i></button>
                    <button class="btn btn-primary" style="min-width: 9px;padding :13px 4px;" ng-click="AlltoB_Estimation()" id="btnAllB_Estimation"><i class="fa fa-angle-double-right"></i></button>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="list-group box-comments" id="list2Estimation" style="height:250px;overflow-x:auto">
                    <a href="javascript:;" class="list-group-item active grouppadding">
                        Selected Approver(s)
                        <input style="color:black;margin-left:5px;" ng-model="search.rApprover" />
                    </a>
                    <span ng-repeat="right in listB_Estimation | filter:search.rApprover">
                        @*| orderBy:'right.productApprover'*@
                        <a href="javascript:;" data-id="{{right.Id}}" class="list-group-item grouppadding">
                            {{right.Approver}}
                            <input id="{{right.Approver}}" ng-model="isCheckedB_Estimation" ng-click="stateBChanged_Estimation(isCheckedB_Estimation, right.Approver)" name="selectedB_Estimation[]" value="{{right.Approver}}" class="pull-right" type="checkbox" ng-true-value="true" ng-false-value="false">
                        </a>
                    </span>
                </div>
            </div>
        </div>
    </div>
    <div class="form-group col-lg-12">
        <div class="col-sm-offset-1 col-sm-11" ng-show="IsModelAlgopage">
            <span>
                <input type="button" value="Prev" ng-click="activateTab(1)" class="btn btn-primary" />
            </span>&nbsp;
            <span ng-show="!editMode">
                <input type="button" value="Save" ng-disabled="adduserform.$invalid" ng-click="InsertStrategy(currency)" class="btn btn-primary" />
            </span>&nbsp;
            <span ng-show="editMode">
                <input type="button" value="Update" ng-click="UpdateStrategy(currency)" class="btn btn-primary" />
            </span>&nbsp;
            <input type="button" value="Cancel" ng-click="Confirmcancel()" class="btn btn-primary cancel" />
        </div>
        <div ng-show="!IsModelAlgopage">
            <span>
                <input type="button" value="Prev" ng-click="activateTab(1)" class="btn btn-primary" />
            </span>&nbsp;
            <span>
                <input type="button" value="Update" ng-click="UpdateChange(currency)" class="btn btn-primary" />
            </span>&nbsp;
            <span>
                <input type="button" value="Save As New Version" ng-click="SaveNewversionStrategy(currency)" class="btn btn-primary" />
            </span>&nbsp;
            <input type="button" value="Cancel" ng-click="Confirmcancel()" class="btn btn-primary cancel" />
        </div>

    </div>
    <div class="form-group">
        <label class="col-sm-6 col-sm-offset-2 control-label label-danger" ng-show="errorinfo.length > 0">{{errorinfo}}</label>
    </div>
</form>
