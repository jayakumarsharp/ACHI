<div ng-show="pageList[0].Page">
    <form novalidate role="form" name="form1">
        <div class="col-md-6">
            <div class="box-body">
                <div class="form-group">
                    <label>Strategy Reference Number</label>
                    <input data-ng-show="false" class="form-control" numberdotonly type="text" maxlength="50" ng-model="ecurrency.RefNumber" />
                    <input class="form-control" type="text" numberdotonly maxlength="50" ng-model="currency.RefNumber" required />
                </div>
                <div class="form-group">
                    <label>Strategy Type</label>
                    <input data-ng-show="false" class="form-control" numberdotonly type="text" maxlength="50" ng-model="ecurrency.Type" />
                    <input class="form-control" type="text" numberdotonly maxlength="50" ng-model="currency.Type" required />
                </div>
                <div class="form-group">
                    <label>Application</label>
                    <select class="form-control" name="ddlApp" ng-model="selectModel.Application" ng-options="item.ApplicationName for item in ApplicationMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Application -</option>
                    </select>

                </div>
                <div class="form-group">
                    <label>Country </label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.Country" ng-change="userfilter()" ng-options="item.CountryName for item in CountryMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Country -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Product Type</label>
                    <select class="form-control" name="ddlProduct" ng-model="selectModel.ProductType" ng-options="item.ProductName for item in ProductMasterList" title="Select Product" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Product -</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="box-body">
                <div class="form-group">
                    <label>Name of Strategy</label>
                    <input data-ng-show="false" class="form-control" numberdotonly type="text" maxlength="50" ng-model="ecurrency.Name" />
                    <input class="form-control" type="text" numberdotonly maxlength="50" ng-model="currency.Name" required />
                </div>

                <div class="form-group">
                    <label>Business Sector</label>

                    <select class="form-control" name="ddlSBU" ng-model="selectModel.BusinessSector" ng-change="userfilter()" ng-options="item.BusinessSectorName for item in BusinessSectorMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Business Master -</option>
                    </select>
                </div>

                <div class="form-group">
                    <label>Region</label>

                    <select class="form-control" name="ddlSBU" ng-model="selectModel.Region" ng-change="userfilter()" ng-options="item.RegionName for item in RegionMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Region -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Strategy Ranking</label>
                    <input data-ng-show="false" class="form-control" numberdotonly type="text" maxlength="50" ng-model="ecurrency.Ranking" />
                    <input class="form-control" type="text" numberdotonly maxlength="50" ng-model="currency.Ranking" required />
                </div>
            </div>
        </div>

    </form>
    <div class="form-group col-lg-12">
        <div class="col-sm-offset-2 col-sm-10">
            <span>
                <input type="button" value="Prev" ng-show="!IsModelAlgopage" ng-click="activateTab(3)" class="btn btn-primary" />
            </span>&nbsp;
            <span>
                <input type="button" value="Next" ng-disabled="form1.$invalid" ng-click="activateTab(1)" class="btn btn-primary" />
            </span>&nbsp;
            <input type="button" value="Cancel" ng-click="Confirmcancel()" class="btn btn-primary cancel" />
        </div>
    </div>
</div>
