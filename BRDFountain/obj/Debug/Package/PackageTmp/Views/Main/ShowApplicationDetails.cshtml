<div ng-show="pageList[0].Page">
    <form novalidate role="form" name="form1">
        @*First Box*@
        <div class="col-md-4">
            <div class="box-body">
                <div class="form-group">
                    <label>FTA Application Code</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.FTAApplicationCode" ng-change="userfilter1()" ng-options="item.FTAApplicationCode for item in FTAApplicationCodeList" title="Select FTAApplicationCode" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select FTAApplication Code -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Business Suffix</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.BusinessSuffix" ng-change="userfilter1()" ng-options="item.BusinessSuffix for item in BusinessSuffixList" title="Select BusinessSuffix" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Business Suffix-</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Child ID</label>
                    <select class="form-control" name="ddlApp" ng-model="selectModel.ChildID" ng-options="item.ChildID for item in ChildIDList" title="Select ChildID" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select ChildID -</option>
                    </select>

                </div>
                <div class="form-group">
                    <label>Country </label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.Country" ng-change="userfilter()" ng-options="item.CountryName for item in CountryMasterList" title="Select Country" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Country -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>FTA Strategy Name</label>
                    <select class="form-control" name="ddlProduct" ng-model="selectModel.FTAStrategyName" ng-options="item.FTAStrategyName for item in FTAStrategyNameList" title="Select FTAStrategyName" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select FTAStrategy Name -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>System Flow</label>
                    <input type="file" file-model="Thanksuploadfile" />
                    @*<select class="form-control" name="ddlProduct" ng-model="selectModel.ProductType" ng-options="item.ProductName for item in ProductMasterList" title="Select Product" style="width:100%;font-size: 0.8em" required>
                            <option value="">- Select Product -</option>
                        </select>*@
                </div>
                <div class="form-group">
                    <label>Strategy type</label>
                    <select class="form-control" ng-model="selectModel.Strategytype" ng-options="item.Strategytype for item in StrategytypeList" title="Select Strategy Type" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Strategy type -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Go-Live date</label>
                    <div class="col-lg-12">
                        <div class='input-group date' datetimepicker ng-model="TransferFrom">
                            <input type='text' class="form-control" />
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @*First Box end*@
        @*Second Box start*@
        <div class="col-md-4">
            <div class="box-body">
                <div class="form-group">
                    <label>FTA Strategy Code</label>
                    <select class="form-control" ng-model="selectModel.FTAStrategyCode" ng-change="userfilter1()" ng-options="item.FTAStrategyCode for item in FTAStrategyCodeList" title="Select FTAStrategyCode" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select FTAStrategy Code -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>FTA Short Code</label>
                    <input type="text" class="form-control" maxlength="100" readonly ng-model="selectModel.FTAShortCode">
                    @*<select class="form-control" name="ddlSBU" ng-model="selectModel.FTAShortCode"  ng-options="item.BusinessSectorName for item in FTAShortCodeList" title="Select FTAShortCode" style="width:100%;font-size: 0.8em" required>
                            <option value="">- Select FTAShort Code -</option>
                        </select>*@
                </div>

                <div class="form-group">
                    <label>Business Line</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.BusinessLine" ng-options="item.BusinessLine for item in BusinessLineList" title="Select BusinessLine" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select BusinessLine-</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>FTA Application Name</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.FTAApplicationName" ng-options="item.FTAApplicationName for item in FTAApplicationNameList" title="Select FTAApplicationName" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select FTAApplicationName -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>FTA Strategy Owner</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.FTAStrategyOwner" ng-options="item.FTAStrategyOwner for item in FTAStrategyOwnerList" title="Select FTAApplicationOwner" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select FTAStrategyOwner -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Application Category</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.ApplicationCategory" ng-options="item.ApplicationCategory for item in ApplicationCategoryList" title="Select Application Category" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select ApplicationCategory -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Venue type</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.Venuetype" ng-change="userfilter()" ng-options="item.Venuetype for item in VenuetypeList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Venuetype-</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Decommissioned Date</label>
                    <div class="col-lg-12">
                        <div class='input-group date' datetimepicker ng-model="TransferFrom">
                            <input type='text' class="form-control" />
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        @*Second Box end*@

        @*3rd Box start*@
        <div class="col-md-4">
            <div class="box-body">
                <div class="form-group">
                    <label>Discretionary Code</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.DiscretionaryCode" ng-change="userfilter1()" ng-options="item.DiscretionaryCode for item in DiscretionaryCodeList" title="Select DiscretionaryCode" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select DiscretionaryCode-</option>
                    </select>
                </div>

                <div class="form-group">
                    <label>Parent ID</label>

                    <select class="form-control" name="ddlSBU" ng-model="selectModel.ParentID" ng-options="item.ParentID for item in ParentIDList" title="Select ParentID" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select ParentID-</option>
                    </select>
                </div>

                <div class="form-group">
                    <label>Region</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.Region" ng-change="userfilter()" ng-options="item.RegionName for item in RegionMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Region -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>FTA Application Owner</label>
                    <select class="form-control" name="ddlSBU" ng-model="selectModel.FTAApplicationOwner" ng-options="item.FTAApplicationOwner for item in FTAApplicationOwnerList" title="Select FTAApplicationOwner" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select FTAApplicationOwner -</option>
                    </select>
                </div>
                @*<div class="form-group">
                        <label>Strategy Description</label>
                        <select class="form-control" name="ddlSBU" ng-model="selectModel.Region" ng-change="userfilter()" ng-options="item.RegionName for item in RegionMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                            <option value="">- Select Region -</option>
                        </select>
                    </div>*@
                <div class="form-group">
                    <label>Priority Score</label>
                    <select class="form-control" name="ddlPriorityScore" ng-model="selectModel.PriorityScore" ng-change="userfilter()" ng-options="item.PriorityScore for item in PriorityScoreList" title="Select PriorityScore" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Priority Score -</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Priority</label>
                    <input type="text" class="form-control" maxlength="100" readonly ng-model="selectModel.Priority">
                    @*<select class="form-control" name="ddlSBU" ng-model="selectModel.Region" ng-change="userfilter()" ng-options="item.RegionName for item in RegionMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                            <option value="">- Select Region -</option>
                        </select>*@
                </div>
                <div class="form-group">
                    <label>Capacity</label>
                    <select class="form-control" name="ddlCapacity" ng-model="selectModel.Capacity" ng-change="userfilter()" ng-options="item.Capacity for item in CapacityList" title="Select Capacity" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Capacity -</option>
                    </select>
                </div>


                <div class="form-group">
                    <label>Decommissioned Evidence</label>
                    <input type="file" file-model="Thanksuploadfile" />
                    @*<select class="form-control" name="ddlSBU" ng-model="selectModel.Region" ng-change="userfilter()" ng-options="item.RegionName for item in RegionMasterList" title="Select Region" style="width:100%;font-size: 0.8em" required>
                            <option value="">- Select Region -</option>
                        </select>*@
                </div>

                @*FTA Owner Attestation*@




            </div>
        </div>
        @*3rd Box end*@
    </form>
    <div class="form-group col-lg-12">
        <div class="col-sm-offset-2 col-sm-10">
            <span>
                <input type="button" value="Prev" ng-show="!IsModelAlgopage" ng-click="activateTab(3)" class="btn btn-primary" />
            </span>&nbsp;
            <span>
                <input type="button" value="Next" ng-click="activateTab(1)" class="btn btn-primary" />
            </span>&nbsp;
            <input type="button" value="Cancel" ng-click="Confirmcancel()" class="btn btn-primary cancel" />
        </div>
    </div>
</div>
<div ng-show="pageList[1].Page">
    <form novalidate role="form" name="form2">

        <div class="col-md-12">
            <div class="box-body">
                <div class="form-group">
                    <label>Description</label>
                    <textarea class="form-control" rows="5" id="comment" placeholder="Enter the description"></textarea>
                </div>

            </div>
            <div class="checkbox">
                <label>
                    <input type="checkbox" value="">
                    <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                    SIGN OFF
                </label>
            </div>

        </div>


    </form>
    <div class="form-group col-lg-12">
        <div class="col-sm-offset-2 col-sm-10">
            <span>
                <input type="button" value="Prev" ng-click="activateTab(0)" class="btn btn-primary" />
            </span>&nbsp;
            @*<span>
                    <input type="button" value="Next" ng-disabled="form2.$invalid" ng-click="activateTab(2)" class="btn btn-primary" />
                </span>&nbsp;*@

            <input type="button" value="Cancel" ng-click="Confirmcancel()" class="btn btn-primary cancel" />
