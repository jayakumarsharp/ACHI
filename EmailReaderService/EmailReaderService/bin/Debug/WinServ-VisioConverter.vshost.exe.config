<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <connectionStrings>
    <add name="ConnStr" providerName="System.Data.SqlClient" connectionString="Data Source=172.20.6.143;Initial Catalog=dbConVisioTool;Persist Security Info=True;User ID=sa;Password=Servion@123;" />
  </connectionStrings>
  <log4net>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="C:/Serviont/ConverterLog/ConverterLog.txt"/>
      <appendToFile value="true"/>
      <maximumFileSize value="1500KB"/>
      <maxSizeRollBackups value="5"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level [%method] - %message%newline"/>
      </layout>
    </appender>
    <!-- Setup the root category, add the appenders and set the default level -->
    <root>
      <level value="Info"/>
      <appender-ref ref="RollingFile"/>
    </root>
  </log4net>
  <appSettings>
    <add key="Filepath" value="D:\Converter\Visio Converter"/>
    <add key="MppFilepath" value="D:\Converter\Project Converter"/>
    <add key="FromMailId" value="dmtest2@servion.in"/>
    <add key="MailID" value="sgsl\dmtest2"/>
    <add key="Password" value="welcome@123"/>
   
    <add key="TimeSec" value="60000"/>
  </appSettings>
</configuration>